#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
    long long n,ans1=0,ans2=0;
    cin >> n;
    string s;
    cin >> s;
    for (int i = 0; i < s.size(); i++)
    {
        if (s[i] == 'A')
        {
            ans1++;
        }
        else
        {
            ans2++;
        }
    }
    if (ans1 > ans2)
    {
        cout << "Anton" << endl;
    }
    else if (ans2 > ans1)
    {
        cout << "Danik" << endl;
    }
    else
    {
        cout << "Friendship" << endl;
    }
}
