#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
    long long n, h,ans=0;
    cin >> n >> h;
    for (int i = 0; i < n; i++)
    {
        long long x;
        cin >> x;
        if (x <= h)
        {
            ans++;
        }
        else
        {
            ans += 2;
        }
    }
    cout << ans << endl;
}
