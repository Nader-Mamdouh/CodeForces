#include <iostream>
#include <vector>
#include <algorithm>
#include <cmath>
#include <iomanip> 
using namespace std;
int main()
{
    long long t;
    cin >> t;
    while (t--)
    {
        long long h, m, g;
        cin >> h >> m;
        h = h * 60;
        g = h + m;
        long long q = 24 * 60;
        cout << q - g << endl;
    }
    return 0;
}
