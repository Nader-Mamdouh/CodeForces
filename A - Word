#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
   
    long long up=0, low=0;
    string s;
    cin >> s;
    for (int i = 0; i < s.size(); i++)
    {
        if (s[i] >= 65 and s[i] <= 90)
            up++;
        if (s[i] >= 97 and s[i] <= 122)
            low++;
    }
    if (up > low)
    {
        cout << "";
        for (int x = 0; x < s.size(); x++)
            putchar(toupper(s[x]));
       
    }
    else
    {
        cout << "";
        for (int x = 0; x < s.size(); x++)
            putchar(tolower(s[x]));
    }

}
