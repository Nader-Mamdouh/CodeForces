#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
    int  a, b,c,d=6;
    cin >> a >> b;
    int mx = max(a, b);
     c = 7 - mx;
     
    if (c ==2||c==3||c==4)
    {
        if (c == 4)
        {
            c = c / 2;
            d = d / 2;
            cout << c << "/" << d << endl;
            return 0;
        }
        else if (c == 3)
        {
            c = c / 3;
            d = d / 3;
            cout << c << "/" << d << endl;
            return 0;
        }
        else
        {
            c = c / 2;
            d = d / 2;
            cout << c << "/" << d << endl;
            return 0;
        }
           
    }
    else if (c==1||c==5)
    {
        cout << c << "/" << d << endl;
       
    }
    else
    {
        cout << 1 << "/" << 1 << endl;
    }
   
}
